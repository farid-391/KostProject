<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kostcoba2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/box_icon_dk"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/original_icon_dk"
        android:supportsRtl="true"
        android:theme="@style/Theme.KostCoba2">
        <activity android:name=".OwnerRegisterActivity"
            android:theme="@style/Theme.KostCobaNoActionBar" />
        <activity android:name=".UserRegisterActivity"
            android:theme="@style/Theme.KostCobaNoActionBar" />
        <activity android:name=".RoleDivideActivity" android:theme="@style/Theme.KostCobaNoActionBar"/>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Theme.KostCobaNoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>